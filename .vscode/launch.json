    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "yarn dev",
            "runtimeExecutable": "yarn",
            "cwd": "${workspaceFolder}",
            "runtimeArgs": [
                "dev"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Import Data",
            "runtimeExecutable": "yarn",
            "cwd": "${workspaceFolder}/api",
            "runtimeArgs": [
                "run",
                "job:import"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Update Data",
            "runtimeExecutable": "yarn",
            "cwd": "${workspaceFolder}/api",
            "runtimeArgs": [
                "run",
                "job:update"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Fullcheck",
            "runtimeExecutable": "yarn",
            "cwd": "${workspaceFolder}",
            "runtimeArgs": [
                "fullcheck"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Import From Inner Identifers",
            "runtimeExecutable": "yarn",
            "cwd": "${workspaceFolder}/api",
            "runtimeArgs": [
                "run",
                "job:import-from-inner-identifiers"
            ],
        }
    ]
}