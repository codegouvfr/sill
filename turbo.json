{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist"]
    },
    "job:import": {},
    "job:update": {},
    "job:import-from-inner-identifiers": {},
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "lint": {},
    "test": {
      "dependsOn": ["typecheck"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "dev": {},
    "dev:db:down": {},
    "dev:db:mig:down": {},
    "dev:db:mig:up": {},
    "dev:db:up": {},
    "dev:db:reset": {},
    "format:check": {},
    "format": {}
  }
}
